PGE	START	0
CC	EQU	3
	WORD	3
AA	LDA	#3
BB	J	AA
DD	EQU	*
EE	STA	AA,X
A0	EQU	DD-6
B0	EQU	CC+EE
C0	EQU	BB-EE
.	A : Absolute	R : Relative
.D0	EQU	CC-BB //Invalid Expression "A-R"
.E0	EQU	DD+BB //Invalid Expression "R+R"
F0	EQU	CC+100
G0	EQU	DD-AA+BB-CC
.H0	EQU	3*AA  //Invalid Expression "A*R"
.I0	EQU	100-AA	 //Invalid Expression "A-R"
	END
